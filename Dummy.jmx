                <stringProp name="script">import java.util.Random

def length = 4  // Random string length
def chars = (&apos;A&apos;..&apos;Z&apos;) + (&apos;a&apos;..&apos;z&apos;) + (&apos;0&apos;..&apos;9&apos;)  // Uppercase, lowercase, numbers
def randomString = (1..length).collect { chars[new Random().nextInt(chars.size())] }.join()

Random rand = new Random()
String randomDigits = (10000 + rand.nextInt(90000)).toString()

// Fetch variables from JMeter (CSV Data Set Config will provide these values)
String name = vars.get(&quot;Name&quot;)
String mobileNumber = vars.get(&quot;Mobile num&quot;)
String email = vars.get(&quot;Email&quot;)
String password = vars.get(&quot;Password&quot;)
String gen = vars.get(&quot;Gender&quot;);

log.info(&quot;Name &quot;+name);

// Generate new values
String fullName = name + randomString;
String prefix = mobileNumber.substring(0, mobileNumber.length() - 5)
String newMobileNumber = prefix + randomDigits

// Modify Email
String[] parts = email.split(&quot;@&quot;)
String username = parts[0] + randomDigits;
String domain = &quot;@&quot; + parts[1]  
String newEmail = username + domain;

// Modify Password
String newPassword = password + randomString;

// Store in JMeter variables
vars.put(&quot;fullName&quot;,fullName);
vars.put(&quot;randomString&quot;, randomString);
vars.put(&quot;newMobileNumber&quot;, newMobileNumber);
vars.put(&quot;NewEmail&quot;, newEmail);
vars.put(&quot;NewPassword&quot;, newPassword);

// Debugging (Optional - Check values in JMeter Logs)
log.info(&quot;FullName: &quot; + fullName);
log.info(&quot;New Email: &quot; + newEmail);
log.info(&quot;New Mobile Number: &quot; + newMobileNumber);
log.info(&quot;New Password: &quot; + newPassword);</stringProp>
