              <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Delete Token and Data" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">// Delete Bearer Token and Customer Number stored inside JMeter properties

String threadNum = String.valueOf(ctx.getThreadNum() + 1);
vars.put(&quot;THREAD_NUM&quot;, threadNum);

String tokenValue = &quot;BEARER_TOKEN_&quot; + threadNum;
String custId = &quot;CUSTOMERNUM_&quot; + threadNum;

props.remove(tokenValue);
props.remove(custId);

log.info(&quot;✅ Bearer token and customer number for user &quot; + threadNum + &quot; removed from properties.&quot;);
</stringProp>
