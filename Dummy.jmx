            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Add Cookie token" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">// Store thread number correctly in &apos;vars&apos; (for local use)
String threadNum = String.valueOf(ctx.getThreadNum() + 1);
vars.put(&quot;THREAD_NUM&quot;, threadNum);  // Ensures THREAD_NUM is available in current thread

// Fetch session ID
String sessionId = vars.get(&quot;SESSION_ID&quot;);

// Check if SESSION_ID is null or empty
if (sessionId == null || sessionId.isEmpty()) {
    log.error(&quot;SESSION_ID is null! Make sure it&apos;s extracted correctly.&quot;);
} else {
    // Store in &apos;props&apos; to share across Thread Groups
    String tokenKey = &quot;BEARER_TOKEN_&quot; + threadNum;
    props.put(tokenKey, sessionId);
    
    // Log the stored value to confirm
    log.info(&quot;✅ Stored Property: &quot; + tokenKey + &quot; = &quot; + props.get(tokenKey));
}
</stringProp>
            </JSR223PostProcessor>
